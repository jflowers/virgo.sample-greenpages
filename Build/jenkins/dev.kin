
"greenpages-dev-build-%branch%" {
	template = 'templates/maven-dev-build.tpl'
	description = "greenpages incremental dev build for the branch %branch%"
	gitUrl = 'https://github.com/jflowers/virgo.sample-greenpages.git'
	gitBranch = '%branch%'
	rootPom = './Product/pom.xml'
	mavenCommandLine = 'install'
	mavenOpts = '-Xmx512m -XX:MaxPermSize=256m'
}